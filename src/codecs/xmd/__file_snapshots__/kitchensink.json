{
  "type": "Article",
  "authors": [
    "Nokome Bentley"
  ],
  "description": "This example is intended to have a variety of different XMarkdown elements in it.\n",
  "title": "Kitchen sink XMarkdown example",
  "content": [
    {
      "type": "Heading",
      "depth": 1,
      "content": [
        "Inline chunks"
      ]
    },
    {
      "type": "Paragraph",
      "content": [
        "Simple ",
        {
          "type": "CodeExpression",
          "programmingLanguage": "r",
          "text": "x * y"
        },
        "."
      ]
    },
    {
      "type": "Paragraph",
      "content": [
        "With parentheses and brackets (and a different language) ",
        {
          "type": "CodeExpression",
          "programmingLanguage": "python",
          "text": "sum(x*y)[1]"
        },
        "."
      ]
    },
    {
      "type": "Paragraph",
      "content": [
        "Just inline code ",
        {
          "type": "CodeFragment",
          "text": "a * 6"
        },
        "."
      ]
    },
    {
      "type": "Heading",
      "depth": 1,
      "content": [
        "Block chunks"
      ]
    },
    {
      "type": "Paragraph",
      "content": [
        "Simple:"
      ]
    },
    {
      "type": "CodeChunk",
      "programmingLanguage": "r",
      "text": "# a comment\ndata = read.table(file)"
    },
    {
      "type": "Paragraph",
      "content": [
        "With options:"
      ]
    },
    {
      "type": "CodeChunk",
      "programmingLanguage": "r",
      "text": "plot(x,y)",
      "meta": {
        "echo": "FALSE",
        "height": "7",
        "width": "8"
      }
    }
  ]
}